<%= render "layouts/errors", :object => @album %>

<%= form_for @album do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :contributors %>
<%= f.text_field :contributors %>
<%= f.label :year_released %>
<%= f.text_field :year_released %>
<%= f.label :price %>
<%= f.text_field :price %>
<%= f.submit "Create Album", class: 'btn btn-warning btn-large' %>
<% end %>
